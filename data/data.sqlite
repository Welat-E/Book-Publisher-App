Table: Users
  Column: user_id, Type: INTEGER
  Column: first_name, Type: VARCHAR
  Column: last_name, Type: VARCHAR
  Column: admin, Type: BOOLEAN
  Column: email, Type: VARCHAR
  Column: password, Type: VARCHAR

Table: Author
  Column: name, Type: VARCHAR
  Column: author_image, Type: TEXT
  Column: birth_date, Type: DATE
  Column: user_id, Type: INTEGER

Table: Book
  Column: book_id, Type: INTEGER
  Column: user_id, Type: INTEGER
  Column: release_date, Type: VARCHAR
  Column: cover_image, Type: VARCHAR
  Column: chapters, Type: INTEGER
  Column: pages, Type: INTEGER

Table: Publisher
  Column: user_id, Type: INTEGER
  Column: publisher_name, Type: VARCHAR

Table: Publication_Details
  Column: book_id, Type: INTEGER
  Column: user_id, Type: INTEGER
  Column: price, Type: NUMERIC
  Column: country, Type: VARCHAR
  Column: units, Type: INTEGER
  Column: link, Type: TEXT
  Column: language, Type: VARCHAR

